<!DOCTYPE html>

<%# Index template %>

<html>

  <head>

    <title>	Movie App </title>

    <% include head %>

  </head>

  <body>

    <% include navbar %>

      <div class="container">

        <div class="row">
          <div class="col-sm-12">
          <h1>Now Playing</h1>
        </div>
        <div class="col-sm-12">
          
          <%# Extract dynamic data from array parsedData %>

          <% parsedData.forEach( (movie) =>  { %>
            <%= movie.title %>
          <% }) %>           
          
        </div>

      </div>

    </div>

  </body>

</html>
